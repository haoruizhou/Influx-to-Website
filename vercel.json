{

    "frontend": "my-react-app",
    "api": "backend",

    "builds": [
    {
        "src": "my-react-app/package.json",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "dist"
        }
    },
    {
        "src": "backend/**/*.js",
        "use": "@vercel/node"
    }
],
    "routes": [
    {
        "src": "/api/(.*)",
        "dest": "/backend/index.js"
    }
]
}